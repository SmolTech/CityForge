---
# Vault file for storing sensitive tunnel credentials
# Encrypt this file with: ansible-vault encrypt vault.yml

tunnel_credentials_vault:
  production: |
    {
      "AccountTag": "your-production-account-tag",
      "TunnelSecret": "your-production-tunnel-secret", 
      "TunnelID": "your-production-tunnel-id"
    }
  
  staging: |
    {
      "AccountTag": "your-staging-account-tag",
      "TunnelSecret": "your-staging-tunnel-secret",
      "TunnelID": "your-staging-tunnel-id"  
    }
  
  development: |
    {
      "AccountTag": "your-development-account-tag",
      "TunnelSecret": "your-development-tunnel-secret",
      "TunnelID": "your-development-tunnel-id"
    }

# Additional secrets
cloudflare_api_token: "your-cloudflare-api-token"
cloudflare_email: "your-cloudflare-email"